{
  "rules": [
    {
      "method": [
        "POST"
      ],
      "dest": "api-webglue.now.sh"
    },
    {
      "pathname": "/socket.io/",
      "dest": "api-webglue.now.sh/socket.io/"
    },
    {
      "pathname": "/socket.io/socket.io.js",
      "dest": "api-webglue.now.sh/socket.io/socket.io.js"
    },
    {
      "dest": "nuxt-webglue.now.sh"
    }
  ]
}
